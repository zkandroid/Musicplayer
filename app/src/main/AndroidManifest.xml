<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.firstapp"
    android:installLocation="auto">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.RESTART_PACKAGES" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyTab" />
        <activity
            android:name=".Courseinfo"
            android:label="@string/title_activity_courseinfo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AlarmAlert"
            android:label="@string/title_activity_alarm_alert"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SendMessage"
            android:label="@string/title_activity_send_message"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Setting"
            android:label="@string/title_activity_setting"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".dialog"
            android:label="@string/title_activity_dialog"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".plan"
            android:label="@string/title_activity_plan"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>